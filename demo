pipeline {
    agent { label 'gol'}
    stages {
        stage('git') {
            agent { label 'gol'}
        steps{
            sh'''
            ls
            java -version
            mvn -version
            mvn package
            ls
            '''
        }
        }
        stage('mvn_package') {
            agent {
                label 'gol2'
            }
        steps{
            sh'''
            pwd
            ls
            '''
        }
        }    
    }
}